<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_customer_service"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.wodm.android.view.newview.AtyTopLayout
        android:id="@+id/back_service"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_90"
        app:BackGroundColor="@color/color_f5c92f"
        app:CustomtitleTextColor="@color/color_3f190c"
        app:CustomtitleTextSize="@dimen/text_size_36_px"
        app:Customtoptitle="@string/service"
        app:leftBackGround="@mipmap/back"
        app:rightType="0" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:src="@mipmap/underdevelopment" />

</RelativeLayout>
